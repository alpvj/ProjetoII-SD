module memoria(count, inX);

input wire[3:0] count;
output reg [3:0] inX;

parameter A = 5;
parameter B = 3;

always begin
	case(count)
		4'b0000 : begin
			in <= A;
			end
			
		4'b0001 : begin
			in <= B;
			end
			
		4'b0010 : begin
			in <= 0;
			end
			
		4'b0011 : begin
			in <= 0;
			end
			
		4'b0100 : begin
			in <= 0;
			end
				
	endcase
end
endmodule